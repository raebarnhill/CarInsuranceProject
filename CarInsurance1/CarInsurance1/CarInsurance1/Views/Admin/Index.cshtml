@using CarInsurance1.Models
@model List<InsuranceQuote>


<div class="row no-gutters m-3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h5 class="m-0">Insurance Quotes</h5>
        </div>
        <div class="card-body">
            @foreach (var insuranceQuote in Model)
            {
                using (Html.BeginForm("Quotes", "Admin", FormMethod.Post))
                {
                   
            <div class="row mb-3">
                <div class="col-2">
                    @insuranceQuote.FirstName

                </div>
                <div class="col">
                    @insuranceQuote.LastName

                </div>
                <div class="col">
                    @insuranceQuote.EmailAddress

                </div>
                <div class="col">
                    @insuranceQuote.quote

                </div>
             
            </div>
                }

            }


        </div>
    </div>
</div>

